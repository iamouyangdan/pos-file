{
  "name": "pos-file",
	"displayName": "pos-file",
	"description": "help us create files more convenient and standardized",
	"version": "1.1.1",
	"publisher": "ouyangdan",
	"engines": {
		"vscode": "^1.54.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onLanguage:typescript"
	],
	"main": "./extension.js",
	
	"contributes": {
		"menus": {
			"explorer/context": [
				{
					"command": "pos-file.createPage",
					"group": "navigation@1"
				},
				{
					"command": "pos-file.createAllModel",
					"group": "navigation@2"
				},
				{
					"command": "pos-file.createView",
					"group": "navigation@3"
				},
				{
					"command": "pos-file.createService",
					"group": "navigation@4"
				},
				{
					"command": "pos-file.createModel",
					"group": "navigation@4"
				},
				{
					"command": "pos-file.createDao",
					"group": "navigation@4"
				},
				{
					"command": "pos-file.createApi",
					"group": "navigation@4"
				},
				{
					"command": "pos-file.createRoute",
					"group": "navigation@4"
				},
				{
					"command": "pos-file.createPopup",
					"group": "navigation@4"
				}			
				
			]
		},
		"commands": [
			{
				"command": "pos-file.createApi",
				"title": "createApi"
			},
			{
				"command": "pos-file.createDao",
				"title": "createDao"
			},
			{
				"command": "pos-file.createModel",
				"title": "createModel"
			},
			{
				"command": "pos-file.createAllModel",
				"title": "createAllModel"
			},
			{
				"command": "pos-file.createService",
				"title": "createService"
			},
			{
				"command": "pos-file.createView",
				"title": "createView"
			},
			{
				"command": "pos-file.createPage",
				"title": "createPage"
			},
			{
				"command": "pos-file.createPopup",
				"title": "createPopup"
			},
			{
				"command": "pos-file.createRoute",
				"title": "createRoute"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
        "@types/vscode": "^1.54.0",
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.4",
        "@types/node": "^12.11.7",
        "eslint": "^7.19.0",
        "glob": "^7.1.6",
        "mocha": "^8.2.1",
        "typescript": "^4.1.3",
        "vscode-test": "^1.5.0"
    }

}
